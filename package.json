{
  "name": "@lastmile/deployment-sdk",
  "version": "1.0.0",
  "description": "Embeddable JavaScript SDK for deploying AI-generated code to production with LastMile",
  "main": "lastmile.js",
  "module": "lastmile.js",
  "types": "lastmile.d.ts",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "lint": "eslint .",
    "build": "npm run build:min",
    "build:min": "terser lastmile.js -o lastmile.min.js -c -m --source-map",
    "serve": "http-server -p 8080 -o example.html"
  },
  "keywords": [
    "deployment",
    "sdk",
    "lastmile",
    "ai",
    "code-deployment",
    "automation",
    "devops",
    "ci-cd"
  ],
  "author": "LastMile Development Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/navin3756/lastmile-deployment-platform.git"
  },
  "bugs": {
    "url": "https://github.com/navin3756/lastmile-deployment-platform/issues"
  },
  "homepage": "https://github.com/navin3756/lastmile-deployment-platform#readme",
  "devDependencies": {
    "eslint": "^8.56.0",
    "http-server": "^14.1.1",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "terser": "^5.27.0"
  },
  "dependencies": {
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  },
  "files": [
    "lastmile.js",
    "lastmile.min.js",
    "lastmile.d.ts",
    "README.md",
    "LICENSE"
  ],
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11"
  ]
}
